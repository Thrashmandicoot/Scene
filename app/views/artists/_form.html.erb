          <%= form_for @artist, class:"form-horizontal" do |f| %>
          <form>
            <div class="form-group">
              <%= f.label :avatar %>
              <%= f.text_field :avatar, class:"form-control" %>
            </div>
            <div class="form-group">
            <%= f.label :name %>
            <%= f.text_field :name, class:"form-control" %>
            </div>
            <div class="form-group">
            <%= f.label :email %>
            <%= f.email_field :email, class:"form-control" %>
            </div>
            <div class="form-group">
            <%= f.label :password %>
            <%= f.password_field :password, class:"form-control" %>
            </div>
            <div class="form-group">
            <%= f.label :address %>
            <%= f.text_field :address, class:"form-control", placeholder:"1234 E Your St." %>
            </div>
            <div class="form-group">
            <%= f.label :facebook %>
            <%= f.text_field :facebook, class:"form-control" %>
            </div>
            <div class="form-group">
            <%= f.label :twitter %>
            <%= f.text_field :twitter, class:"form-control" %>
            </div>
            <div class="form-group">
            <%= f.label :website %>
            <%= f.text_field :website, class:"form-control" %>
            </div>
            <div class="form-group">
            <%= f.label :bio %>
            <%= f.text_field :bio, class:"form-control" %>
            </div>

            <%= f.submit 'Create Artist', class: 'btn btn-primary' %>

            <% end %>
          </form>
