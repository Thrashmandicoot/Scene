<%= form_for @artist do |f| %>

<%= f.label :avatar %>
<%= f.text_field :avatar %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :email %>
<%= f.email_field :email %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.label :address %>
<%= f.text_field :address, :placeholder => "Street, City, State, Zipcode"%>

<%= f.label :facebook %>
<%= f.text_field :facebook %>

<%= f.label :twitter %>
<%= f.text_field :twitter %>

<%= f.label :website %>
<%= f.text_field :website %>

<%= f.label :bio %>
<%= f.text_field :bio %>

<%= f.submit %>

<% end %>
