<h1>Pieces#index</h1>
<p>Find me in app/views/pieces/index.html.erb</p>

<% @pieces.each do |piece| %>
  <ul>
    <li>
      <div><%= link_to image_tag(piece.img), artist_piece_path(piece.artist_id, piece) %></div>
      <p>Title: <%= piece.title %></p>
      <p><%= piece.description %></p>
    </li>
  </ul>
<% end %>


